{
  "name": "my-lwr-app",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules __lwr_cache__ site",
    "dev": "lwr dev",
    "dev:compat": "lwr dev --mode compat",
    "build": "lwr build --clean",
    "deploy": "gh-pages -d site",
    "build:prod-compat": "lwr build --clean --mode prod-compat",
    "start": "lwr start",
    "stage": "yarn build && yarn start",
    "stage:prod-compat": "yarn build:prod-compat && yarn start"
  },
  "dependencies": {
    "lwc": "latest",
    "lwr": "0.17.2",
    "my-lwr-app": "file:"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.16.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
